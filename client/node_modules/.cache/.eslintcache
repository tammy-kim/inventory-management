(\"\");\r\n  const [inputDescription, setInputDescription] = useState(\"\");\r\n  const [inputPrice, setInputPrice] = useState(\"\");\r\n  const [inputImage, setInputImage] = useState(\"\");\r\n  const [inputSize, setInputSize] = useState(\"small\");\r\n\r\n  const generateUniqueId = () => {\r\n    return Math.floor(Math.random() * 1000000);\r\n  };\r\n\r\n  const nameChangeHandler = (e) => {\r\n    setInputName(e.target.value);\r\n  };\r\n\r\n  const descriptionChangeHandler = (e) => {\r\n    setInputDescription(e.target.value);\r\n  };\r\n\r\n  const priceChangeHandler = (e) => {\r\n    setInputPrice(e.target.value);\r\n  };\r\n\r\n  const imageChangeHandler = (e) => {\r\n    setInputImage(e.target.value);\r\n  };\r\n\r\n  const sizeChangeHandler = (e) => {\r\n    setInputSize(e.target.value);\r\n  };\r\n\r\n  const submitHandler = (e) => {\r\n    e.preventDefault();\r\n    const item = {\r\n      id: generateUniqueId().toString(),\r\n      name: inputName,\r\n      description: inputDescription,\r\n      price: inputPrice,\r\n      image: inputImage,\r\n      size: inputSize,\r\n    };\r\n    props.saveItem(item);\r\n\r\n    fetch(\"http://localhost:3001/items\", {\r\n      method: \"POST\",\r\n      headers: {\r\n        \"Content-Type\": \"application/json\",\r\n      },\r\n      body: JSON.stringify(item),\r\n    });\r\n\r\n    setInputName(\"\");\r\n    setInputDescription(\"\");\r\n    setInputPrice(\"\");\r\n    setInputImage(\"\");\r\n    setInputSize(\"small\");\r\n  };\r\n\r\n  const clearHandler = (e) => {\r\n    setInputName(\"\");\r\n    setInputDescription(\"\");\r\n    setInputPrice(\"\");\r\n    setInputImage(\"\");\r\n    setInputSize(\"small\");\r\n  };\r\n\r\n  useEffect(() => {\r\n    fetch(\"http://localhost:3001/items\")\r\n      .then((r) => r.json())\r\n      .then((data) => {\r\n        for (const item of data) {\r\n          props.saveItem({\r\n            id: item._id,\r\n            name: item.name,\r\n            description: item.description,\r\n            price: item.price,\r\n            image: item.image,\r\n            size: item.size,\r\n          });\r\n        }\r\n      });\r\n  }, []);\r\n\r\n  return (\r\n    <div className=\"create\">\r\n      <h2>Add a new item</h2>\r\n      <form onSubmit={submitHandler}>\r\n        <label>Name: </label>\r\n        <input\r\n          type=\"text\"\r\n          required\r\n          value={inputName}\r\n          onChange={nameChangeHandler}\r\n        />\r\n        <br />\r\n        <label>Description: </label>\r\n        <input\r\n          type=\"text\"\r\n          required\r\n          value={inputDescription}\r\n          onChange={descriptionChangeHandler}\r\n        />\r\n        <br />\r\n        <label>Price: </label>\r\n        <input\r\n          type=\"text\"\r\n          required\r\n          value={inputPrice}\r\n          onChange={priceChangeHandler}\r\n        />\r\n        <br />\r\n        <label>Image URL: </label>\r\n        <input\r\n          type=\"text\"\r\n          required\r\n          value={inputImage}\r\n          onChange={imageChangeHandler}\r\n        />\r\n        <br />\r\n        <label>Size: </label>\r\n        <select value={inputSize} onChange={sizeChangeHandler}>\r\n          <option value=\"small\">small</option>\r\n          <option value=\"medium\">medium</option>\r\n          <option value=\"large\">large</option>\r\n        </select>\r\n        <br />\r\n        <br />\r\n        <button type=\"submit\">Add Item</button>\r\n        <button type=\"button\" onClick={clearHandler}>\r\n          Clear\r\n        </button>\r\n      </form>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default InventoryForm;\r\n","C:\\Users\\Tammy\\Documents\\CPSC 455\\inventory-management\\client\\src\\components\\Item.js",[],[],"C:\\Users\\Tammy\\Documents\\CPSC 455\\inventory-management\\client\\src\\reducers\\itemReducer.js",[],[],"C:\\Users\\Tammy\\Documents\\CPSC 455\\inventory-management\\client\\src\\components\\Modal.js